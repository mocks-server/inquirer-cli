{
  "name": "@mocks-server/plugin-inquirer-cli",
  "version": "1.3.5",
  "description": "Plugin for Mocks server. Displays an interactive CLI",
  "keywords": [
    "mocks-server-plugin",
    "interactive",
    "cli",
    "inquirer",
    "administration",
    "testing",
    "development"
  ],
  "author": "Javier Brea",
  "license": "Apache-2.0",
  "repository": "https://github.com/mocks-server/plugin-inquirer-cli",
  "homepage": "https://www.mocks-server.org",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "src",
    "index.js"
  ],
  "main": "index.js",
  "scripts": {
    "lint": "eslint *.js src test",
    "lint-staged": "lint-staged",
    "test": "jest",
    "test:acceptance": "jest --config=jest.acceptance.config.js --runInBand",
    "test:coverage": "npm run test",
    "test:ci": "npm run test && npm run test:acceptance",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "peerDependencies": {
    "@mocks-server/core": ">=1.3.0"
  },
  "dependencies": {
    "chalk": "4.1.0",
    "inquirer": "7.3.3",
    "inquirer-autocomplete-prompt": "1.0.2",
    "lodash": "4.17.20"
  },
  "devDependencies": {
    "@mocks-server/core": "1.4.8",
    "coveralls": "3.0.9",
    "eslint": "7.6.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "fs-extra": "9.0.1",
    "husky": "4.2.5",
    "is-promise": "4.0.0",
    "jest": "26.4.0",
    "lint-staged": "10.2.11",
    "prettier": "2.0.5",
    "request": "2.88.2",
    "request-promise": "4.2.6",
    "sinon": "9.0.3",
    "strip-ansi": "6.0.0",
    "tree-kill-sync": "1.0.0"
  },
  "lint-staged": {
    "src/**/*.js": "eslint",
    "test/**/*.js": "eslint",
    "*.js": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "engines": {
    "node": "12.x || 14.x"
  }
}
